        <value>(function execute(inputs, outputs) &#13;
{&#13;
	try {&#13;
		gs.info(current);&#13;
		var gr = new GlideRecord('x_1509365_mktest_carboniteintegrationconfiguration');&#13;
		gr.query();&#13;
		while(gr.next())&#13;
		{&#13;
			var scheduledJobName = 'CarboniteTestDataPull';&#13;
			var scheduledJob = new GlideRecord('sysauto');&#13;
			scheduledJob.addQuery("name", scheduledJobName);&#13;
			scheduledJob.addQuery("sys_scope", gs.getCurrentApplicationId());&#13;
			scheduledJob.query();&#13;
&#13;
			if (scheduledJob.next())&#13;
			{&#13;
				scheduledJob.run_period = new GlideDuration(gr.schedulefrequency);&#13;
				scheduledJob.active = true;&#13;
				scheduledJob.run_start = new GlideDateTime();&#13;
				scheduledJob.update();&#13;
				gs.info('Scheduled Job frequency updated successfully.');&#13;
			}&#13;
			else&#13;
			{&#13;
				gs.info('Scheduled Job not found.');&#13;
			}&#13;
		}&#13;
	}catch(ex){&#13;
		throw ex;&#13;
	}&#13;
}&#13;
)(inputs, outputs);&#13;
	</value>
